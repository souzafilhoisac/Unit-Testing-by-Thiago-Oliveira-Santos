{
	"name": "testesunitarios",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"prebuild": "del-cli dist",
		"build": "nest build",
		"start:dev": "nest start --watch",
		"test": "jest test/unit runInBand",
		"test:coverage": "jest test/unit --coverage --runInBand",
		"test:e2e": "jest test/e2e --runInBand",
		"lint": "npm run lint:format && npm run lint:style",
		"lint:fix": "npm run lint:format:fix && npm run lint:style:fix",
		"lint:format": "prettier --check \"{src,test}/**/*.{js,ts}\" \"*.json\"",
		"lint:format:fix": "prettier --write \"{src,test}/**/*.{js,ts}\" \"*.json\"",
		"lint:style": "eslint src/** test/**",
		"lint:style:fix": "eslint src/** test/** --fix",
		"prepare": "husky install"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@nestjs/cli": "^9.1.8",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.1",
		"@typescript-eslint/parser": "^5.48.1",
		"del-cli": "^5.0.0",
		"eslint": "^8.31.0",
		"eslint-config-standard-with-typescript": "^26.0.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-n": "^15.6.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.3",
		"jest": "^29.3.1",
		"jest-callslike": "^0.2.1",
		"jest-extended": "^3.2.3",
		"ts-jest": "^29.0.4",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"prettier": "^2.8.2"
	}
}
